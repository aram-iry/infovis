            ],
            "type": "buttons",
            "x": 0.1,
            "y": 0,
            "bgcolor": "rgba(255,255,255,0.8)",
            "bordercolor": "#444"
        }]
    ),
    frames=frames
)

# Add explanatory annotation
fig_slider.add_annotation(
    x=0.5,
    y=-0.1,
    xref="paper",
    yref="paper",
    text="Color scale automatically focuses on 95% of data range for each year",
    showarrow=False,
    font=dict(size=12, color="#444")
)

fig_total.show()
fig_slider.show()
